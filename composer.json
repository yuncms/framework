{
  "name": "yuncms/framework",
  "description": "The Yuncms Framework.",
  "keywords": [
    "framework",
    "yuncms"
  ],
  "license": "MIT",
  "homepage": "https://www.yuncms.net",
  "type": "yii2-extension",
  "support": {
    "issues": "https://github.com/yuncms/framework/issues",
    "source": "https://github.com/yuncms/framework"
  },
  "authors": [
    {
      "name": "XuTongle",
      "email": "xutongle@yuncms.net"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-curl": "*",
    "ext-dom": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-pcre": "*",
    "ext-pdo": "*",
    "ext-zip": "*",
    "guzzlehttp/guzzle": "~6.3",
    "league/flysystem": "~1.0",
    "league/flysystem-cached-adapter": "^1.0",
    "danielstjules/stringy": "~3.1.0",
    "jakeasmith/http_build_url": "~1.0",
    "creocoder/yii2-taggable": "~2.0",
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.1.0",
    "yiisoft/yii2-queue": "~2.0.0",
    "yiisoft/yii2-jui": "~2.0",
    "yiisoft/yii2-authclient": "~2.1.0",
    "yiisoft/yii2-imagine": "~2.1",
    "yiisoft/yii2-sphinx": "~2.0.0",
    "yiisoft/yii2-redis": "~2.0.0",
    "yii2tech/ar-position": "~1.0",
    "2amigos/qrcode-library": "~1.1.2",
    "xutl/yii2-wechat": "~2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "yiisoft/yii2-debug": "~2.0.0",
    "yiisoft/yii2-gii": "~2.0.0",
    "league/flysystem-aws-s3-v3": "~1.0",
    "league/flysystem-azure": "~1.0",
    "league/flysystem-cached-adapter": "~1.0",
    "league/flysystem-dropbox": "~1.0",
    "league/flysystem-gridfs": "~1.0",
    "league/flysystem-rackspace": "~1.0",
    "league/flysystem-replicate-adapter": "~1.0",
    "league/flysystem-sftp": "~1.0",
    "league/flysystem-webdav": "~1.0",
    "league/flysystem-ziparchive": "~1.0",
    "xxtime/flysystem-aliyun-oss": "~1.2",
    "overtrue/flysystem-qiniu": "~1.0",
    "jellybool/flysystem-upyun": "~1.0",
    "freyo/flysystem-qcloud-cos-v3": "~2.0",
    "freyo/flysystem-qcloud-cos-v4": "~2.0",
    "freyo/flysystem-qcloud-cos-v5": "~1.0",
    "yuncms/mq-aliyun": "~1.0",
    "yuncms/mq-qcloud": "~1.0",
    "jpush/jpush": "~3.5",
    "xutl/yii2-aliyun": "~2.0",
    "yuncms/composer": "2.0.x-dev"
  },
  "suggest": {
    "ext-imagick": "Adds support for more image processing formats and options.",
    "ext-intl": "Adds rich internationalization support."
  },
  "provide": {
    "bower-asset/jquery": "3.2.*@stable | 3.1.*@stable | 2.2.*@stable | 2.1.*@stable | 1.11.*@stable | 1.12.*@stable",
    "bower-asset/inputmask": "~3.2.2 | ~3.3.5",
    "bower-asset/punycode": "1.3.*",
    "bower-asset/yii2-pjax": "~2.0.1",
    "bower-asset/font-awesome": "4.7.*@stable"
  },
  "autoload": {
    "classmap": [
      "migrations"
    ],
    "psr-4": {
      "yuncms\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "yuncms\\tests\\": "tests/"
    }
  },
  "extra": {
    "yuncms": {
      "id": "framework",
      "migrationPath": "@vendor/yuncms/framework/migrations"
    },
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "repositories": [
    {
      "packagist": false
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
