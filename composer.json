{
  "name": "yuncms/framework",
  "description": "The Yuncms Framework.",
  "keywords": [
    "framework",
    "yuncms"
  ],
  "license": " LGPL-3.0",
  "homepage": "https://www.yuncms.net",
  "type": "yii2-extension",
  "support": {
    "issues": "https://github.com/yuncms/framework/issues",
    "source": "https://github.com/yuncms/framework"
  },
  "authors": [
    {
      "name": "XuTongle",
      "email": "xutongle@yuncms.net"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-mbstring": "*",
    "ext-ctype": "*",
    "lib-pcre": "*",
    "yuncms/composer": "~1.0.0",
    "yuncms/filesystem": "~1.0.0",
    "guzzlehttp/guzzle": "~6.3",
    "danielstjules/stringy": "~3.1.0",
    "jakeasmith/http_build_url": "~1.0",
    "creocoder/yii2-taggable": "~2.0",
    "nesbot/carbon": "~1.0",
    "yiisoft/yii2": "~2.0.0",
    "yiisoft/yii2-swiftmailer": "~2.1.0",
    "yiisoft/yii2-queue": "~2.0.0",
    "yiisoft/yii2-jui": "~2.0",
    "yiisoft/yii2-authclient": "~2.1.0",
    "yiisoft/yii2-imagine": "~2.1.0",
    "yiisoft/yii2-sphinx": "~2.0.0",
    "yiisoft/yii2-redis": "~2.0.0",
    "yiisoft/yii2-bootstrap": "~2.0.0",
    "yiisoft/yii2-httpclient": "~2.0.0",
    "yii2tech/ar-position": "~1.0",
    "2amigos/qrcode-library": "~1.1.2",
    "xutl/yii2-wechat": "~2.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "yiisoft/yii2-debug": "~2.0.0",
    "yiisoft/yii2-gii": "~2.0.0",
    "yuncms/mq-aliyun": "~1.0",
    "yuncms/mq-qcloud": "~1.0",
    "jpush/jpush": "~3.5",
    "xutl/yii2-aliyun": "~2.0"
  },
  "suggest": {
    "ext-imagick": "Adds support for more image processing formats and options.",
    "ext-intl": "Adds rich internationalization support."
  },
  "provide": {
    "bower-asset/jquery": "3.2.*@stable | 3.1.*@stable | 2.2.*@stable | 2.1.*@stable | 1.11.*@stable | 1.12.*@stable",
    "bower-asset/inputmask": "~3.2.2 | ~3.3.5",
    "bower-asset/punycode": "1.3.*",
    "bower-asset/yii2-pjax": "~2.0.1",
    "bower-asset/font-awesome": "4.7.*@stable",
    "bower-asset/jquery-ui": "~1.12.1"
  },
  "autoload": {
    "classmap": [
      "migrations"
    ],
    "psr-4": {
      "yuncms\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "yuncms\\tests\\": "tests/"
    }
  },
  "extra": {
    "yuncms": {
      "id": "framework"
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "packagist": false
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
